 common.mk |    6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/common.mk b/common.mk
index 42d1bf6..9a8b37b 100644
--- a/common.mk
+++ b/common.mk
@@ -167,7 +167,7 @@ FM := qcom.fmradio
 FM += libqcomfm_jni
 
